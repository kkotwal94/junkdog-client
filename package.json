    "moment": "^2.22.1",
    "numeral": "^2.0.6",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-apollo": "^2.1.4",
    "react-dom": "^16.4.0",
    "react-helmet": "^5.2.0",
    "react-jss": "^8.4.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
